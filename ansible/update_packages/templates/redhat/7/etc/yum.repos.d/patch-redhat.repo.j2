[patch-rhel-7-server-base]
name=Patch - Red Hat Enterprise Linux Server Base(v. 6/7 for x86_64)
baseurl=http://repo-linux.tcsbank.ru/rhel/$releasever/$basearch/
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release
enabled=1
gpgcheck=0
repo_gpgcheck=0
skip_if_unavailable=True
proxy=_none_

[patch-rhel-7-server-extras]
name=Patch - Red Hat Enterprise Linux Server Extras(v. 6/7 for x86_64)
