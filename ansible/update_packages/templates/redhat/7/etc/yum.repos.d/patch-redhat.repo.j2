[patch-epel]
name=Patch - Extra Packages for Enterprise Linux (v. 6/7 for x86_64)
baseurl=http://repo-linux.tcsbank.ru/repository/epel/$releasever/$basearch/
enabled=1
gpgcheck=0
repo_gpgcheck=0
