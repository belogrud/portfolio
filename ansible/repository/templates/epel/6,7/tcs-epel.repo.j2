[tcs-epel]
name=Extra Packages for Enterprise Linux $releasever - $basearch
baseurl=http://repo-linux.tcsbank.ru/repository/epel/$releasever/$basearch/
# baseurl=http://repo-linux.tcsbank.ru/epel/$releasever/$basearch
failovermethod=priority
enabled=1
gpgcheck=0
repo_gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-{{ ansible_distribution_major_version }}
skip_if_unavailable=True
proxy=_none_


[tcs-epel-debuginfo]
name=Extra Packages for Enterprise Linux $releasever - $basearch - Debug
baseurl=http://repo-linux.tcsbank.ru/repository/epel/$releasever/$basearch/debug/
# baseurl=http://repo-linux.tcsbank.ru/epel/$releasever/$basearch/debug
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-{{ ansible_distribution_major_version }}
skip_if_unavailable=True
proxy=_none_


[tcs-epel-source]
name=Extra Packages for Enterprise Linux $releasever - $basearch - Source
baseurl=http://repo-linux.tcsbank.ru/repository/epel/$releasever/SRPMS/
# baseurl=http://repo-linux.tcsbank.ru/epel/$releasever/SRPMS
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-{{ ansible_distribution_major_version }}
skip_if_unavailable=True
proxy=_none_
